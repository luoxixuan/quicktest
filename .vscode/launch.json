    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cocos-Launch",
            "type": "lua",
            "request": "launch",
            "runtimeType": "Cocos2",
            "localRoot": "${workspaceRoot}",
            "mainFile": "src/main.lua",
            "commandLine": [
                "-workdir ${workspaceRoot}",
                "-file src/main.lua",
                "-remote-debug"
            ],
            "port": 7003,
            "exePath": "${workspaceRoot}/frameworks/runtime-src/proj.win32/Debug.win32/quicktest.exe",
            "printType": 1
        }
    ]
}